f_num = float(input("введите первое число>>"))
oper = input('Введите операцию>>')
s_num = float(input("введите второе число>>"))
if oper == '+':
    print(f_num + s_num)
elif oper == '-':
    print(f_num - s_num)
if oper == '*':
    print(f_num * s_num)
elif oper == '/':
    print(f_num / s_num)
    


